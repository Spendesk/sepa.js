{
    "rules": {
        "curly": [
            1,
            "multi-line"
        ],
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [0]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true,
    },
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 8,
    },
    "plugins": [
        "@typescript-eslint",
        "jest",
    ]
}
